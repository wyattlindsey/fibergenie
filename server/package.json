  "bugs": {
    "url": "https://github.com/wyattlindsey/fibergenie/issues"
  },
  "homepage": "https://github.com/wyattlindsey/fibergenie#readme",
  "dependencies": {
    "dot-prop": "^4.2.0",
    "express": "^4.16.3",
    "gm": "^1.23.1",
    "multer": "^1.3.0",
    "ndv": "^1.10.3",
    "pdf2pic": "^1.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.17.5"
  }
